   <children>
      <Text layoutX="250.0" layoutY="100.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register as">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button layoutX="250.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleStudentRegistration" text="Student" />
      <Button layoutX="250.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleTeacherRegistration" text="Teacher" />
   </children>
</AnchorPane>
