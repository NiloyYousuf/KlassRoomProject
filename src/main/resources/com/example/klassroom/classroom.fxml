<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.klassroom.ClassroomMakerController">
    <Label text="Create Classroom" />
    <TextField fx:id="classroomCode" promptText="Classroom Code" />
    <TextField fx:id="classroomPassword" promptText="Classroom Password" />
    <Button text="Create Classroom" onAction="#createClassroom" />
</VBox>
