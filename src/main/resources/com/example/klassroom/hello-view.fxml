<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.klassroom.LoginController">
    <Button fx:id="studentButton" layoutX="125.0" layoutY="50.0" onAction="#loginAsStudent" prefWidth="150" text="Login as Student" />
    <Button fx:id="teacherButton" layoutX="125.0" layoutY="100.0" onAction="#loginAsTeacher" prefWidth="150" text="Login as Teacher" />
    <AnchorPane fx:id="content" layoutX="0.0" layoutY="150.0" prefHeight="150" prefWidth="400" />
</AnchorPane>
