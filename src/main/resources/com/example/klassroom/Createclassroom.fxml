<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="206.0" prefWidth="225.0" spacing="10" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.teacher.ClassroomMakerController">
    <Label text="Create Classroom" />
    <TextField fx:id="classroomCode" promptText="Classroom Code" />
    <TextField fx:id="classroomPassword" promptText="Classroom Password" />
   <TextField fx:id="classroomname" promptText="Classroom Name" />
    <Button onAction="#createClassroom" text="Create Classroom" />
</VBox>
